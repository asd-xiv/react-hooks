{
  "name": "@asd14/react-hooks",
  "version": "0.0.1",
  "description": "Various React Hooks",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/asd-xiv/react-hooks#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asd-xiv/react-hooks.git"
  },
  "bugs": {
    "url": "https://github.com/asd-xiv/react-hooks/issues"
  },
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidmt"
  },
  "main": "dist/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "----UTIL": "",
    "setup:clean": "rm -rf ./node_modules",
    "setup": "npm run setup:clean && npm i && npm audit fix",
    "setup:ci": "npm ci && npm audit fix",
    "update": "npm-check --update",
    "update-force": "npm-check --update-all",
    "----BUILD": "",
    "build:clean": "rm -rf ./dist",
    "build": "npm run build:clean && babel src --out-dir dist",
    "----LINT": "",
    "lint:md": "markdownlint *.md",
    "lint:js": "eslint --ext .jsx,.js,.json --quiet src",
    "lint": "npm run lint:md && npm run lint:js",
    "----TEST": "",
    "test": "riteway -r @babel/register 'src/**/*.test.js' 'src/**/*.test.jsx' | tap-nirvana",
    "tdd": "nodemon -e js,jsx,json --watch src --exec 'npm test'",
    "----PUBLISH": "",
    "publish": "semantic-release"
  },
  "dependencies": {
    "@asd14/fetch-browser": "^2.0.0",
    "@asd14/m": "^5.1.0",
    "@babel/runtime-corejs3": "^7.13.8",
    "classnames": "^2.2.6",
    "core-js": "^3.9.1",
    "cuid": "^2.1.8",
    "debug": "^4.1.1",
    "luxon": "^1.26.0",
    "qs": "^6.9.6",
    "socket.io-client": "^3.1.2"
  },
  "peerDependencies": {
    "react": "^17.*",
    "react-dom": "^17.*",
    "react-redux": "^7.*",
    "react-router-dom": "^5.*"
  },
  "devDependencies": {
    "@asd14/eslint-config": "^5.29.0",
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.13.8",
    "@babel/plugin-transform-react-constant-elements": "^7.12.13",
    "@babel/plugin-transform-react-display-name": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@babel/register": "^7.13.8",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-jsx-remove-data-test-id": "^3.0.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-no-inferred-method-name": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^28.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.2.0",
    "http-server": "^0.12.3",
    "markdownlint-cli": "^0.26.0",
    "nodemon": "^2.0.7",
    "npm-check": "^5.9.2",
    "prettier": "^2.2.1",
    "riteway": "^6.2.1",
    "semantic-release": "^17.4.0",
    "tap-nirvana": "^1.1.0",
    "testcafe": "^1.11.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.0.0-beta.0"
  }
}

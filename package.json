{
  "name": "@asd14/react-hooks",
  "version": "0.0.1",
  "description": "Various React Hooks",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/asd-xiv/react-hooks#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asd-xiv/react-hooks.git"
  },
  "bugs": {
    "url": "https://github.com/asd-xiv/react-hooks/issues"
  },
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidmt"
  },
  "main": "dist/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "----UTIL": "",
    "setup": "npm ci --include=dev",
    "update": "ncu --interactive",
    "postupdate": "npm audit fix",
    "----BUILD": "",
    "prebuild": "rm -rf ./dist",
    "build": "babel src --out-dir dist",
    "build:watch": "nodemon -e js,jsx,json --watch src --exec 'npm run build'",
    "----LINT": "",
    "lint:md": "markdownlint '*.md'",
    "lint:js": "eslint --ext .jsx,.js,.json --quiet src",
    "lint": "npm run lint:md && npm run lint:js",
    "----TEST": "",
    "test": "riteway -r @babel/register 'src/**/*.test.js' 'src/**/*.test.jsx' | tap-nirvana",
    "tdd": "nodemon -e js,jsx,json --watch src --exec 'npm test'",
    "----PUBLISH": "",
    "publish": "semantic-release"
  },
  "dependencies": {
    "@asd14/fetch-browser": "^2.3.0",
    "@asd14/m": "^8.1.0",
    "@babel/runtime": "^7.20.7",
    "classnames": "^2.3.2",
    "cuid": "^2.1.8",
    "debug": "^4.3.4",
    "luxon": "^3.2.1",
    "qs": "^6.11.0",
    "react": "^18.*",
    "react-dom": "^18.*",
    "react-redux": "^8.*",
    "react-router-dom": "^6.*",
    "socket.io-client": "^4.5.4"
  },
  "peerDependencies": {
    "react": "^17.*",
    "react-dom": "^17.*",
    "react-redux": "^7.*",
    "react-router-dom": "^5.*"
  },
  "devDependencies": {
    "@asd14/eslint-config": "^8.3.0",
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-react-constant-elements": "^7.20.2",
    "@babel/plugin-transform-react-display-name": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "babel-loader": "^9.1.2",
    "babel-plugin-jsx-remove-data-test-id": "^3.0.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-no-inferred-method-name": "^2.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.1",
    "markdownlint-cli": "^0.33.0",
    "nodemon": "^2.0.20",
    "npm-check-updates": "^16.6.2",
    "prettier": "^2.8.2",
    "riteway": "^7.0.0",
    "semantic-release": "^20.0.2",
    "tap-nirvana": "^1.1.0",
    "testcafe": "^2.2.0",
    "url-loader": "^4.1.1",
